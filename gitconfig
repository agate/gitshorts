[color]
  ui          = true
  status      = auto
  branch      = auto
  diff        = auto
  interactive = auto
[merge]
  tool = vimdiff
[core]
  editor    = vim
  quotepath = false
[alias]
  ls = ls-file
  st = status
  ci = commit -a
  co = checkout
  cl = clone
  rb = rebase
  ps = push
  pl = pull
  rp = pull --rebase
  df = diff
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
